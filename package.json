{
  "name": "yuulog",
  "version": "1.0.0",
  "description": "yuu blog service",
  "main": "./index.js",
  "types": "./dist/types/main.d.ts",
  "scripts": {
    "start": "npm run build && set NODE_ENV=production && node index.js",
    "dev": "set NODE_ENV=development && ts-node --files --project tsconfig.json src/main.ts",
    "build": "npm run clean && tsc --build tsconfig.json",
    "build:watch": "npm run clean && tsc --build tsconfig.json -w",
    "clean": "node scripts/clean",
    "prepublishOnly": "npm run build",
    "postpublish": "npm run clean",
    "postinstall": "node scripts/postinstall.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TokisakiYuu/Yuulog.git"
  },
  "keywords": [
    "blog"
  ],
  "author": "tokisakiyuu",
  "license": "MIT",
  "engines": {
    "node": ">=10.0.0"
  },
  "bugs": {
    "url": "https://github.com/TokisakiYuu/Yuulog/issues"
  },
  "homepage": "https://github.com/TokisakiYuu/Yuulog#readme",
  "dependencies": {
    "selfsigned": "^1.10.8"
  },
  "devDependencies": {
    "@types/node": "^14.14.6",
    "rimraf": "^3.0.2",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.3"
  }
}
